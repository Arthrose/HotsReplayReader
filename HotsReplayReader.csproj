<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
	  <TargetFramework>net9.0-windows</TargetFramework>
	  <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationIcon>HotsReplayReader.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Classes\**" />
    <EmbeddedResource Remove="Classes\**" />
    <None Remove="Classes\**" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="NewFolder\HotsReplayReader.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="WebView2.Runtime.X64" Version="132.0.2957.140" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="NewFolder\abilitytalents.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>AbilityTalents.resx</DependentUpon>
    </Compile>
    <Compile Update="NewFolder\heroesIcon.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>HeroesIcon.resx</DependentUpon>
    </Compile>
    <Compile Update="NewFolder\hotsEmoticons.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>HotsEmoticons.resx</DependentUpon>
    </Compile>
    <Compile Update="NewFolder\hotsResources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>HotsResources.resx</DependentUpon>
    </Compile>
    <Compile Update="NewFolder\MatchAwardsImg.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>MatchAwardsImg.resx</DependentUpon>
    </Compile>
    <Compile Update="NewFolder\minimapIcons.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>MinimapIcons.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="NewFolder\AbilityTalents.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>abilitytalents.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="NewFolder\HeroesIcon.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>heroesIcon.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="NewFolder\HotsEmoticons.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>hotsEmoticons.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="NewFolder\HotsResources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>hotsResources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="NewFolder\MatchAwardsImg.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>MatchAwardsImg.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="NewFolder\MinimapIcons.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>minimapIcons.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Heroes.Icons\Heroes.Icons\Heroes.Icons.csproj" />
    <ProjectReference Include="..\Heroes.StormReplayParser\Heroes.StormReplayParser\Heroes.StormReplayParser.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="obj\Debug\net9.0-windows\" />
  </ItemGroup>
</Project>